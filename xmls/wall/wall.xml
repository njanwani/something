<mujoco model="wall">
  <compiler angle="radian" autolimits="true"/>  
  <visual>
    <!-- <quality shadowsize="8192"/> -->
  </visual>

  <asset>
    <texture name="wall" file="assets/wall.png" type="2d"/>
    <material name="wall" texture="wall"texrepeat="1 2"/>
    <material name="black2" rgba="0.25 0.25 0.25 1"/>
  </asset>

  <default>
    <default class="wall_collision">
      <geom type="box" group="3"/>
    </default>
    <default class="rung">
      <geom type="box" size="0.15 0.1 0.02" material="black2"/>
    </default>
  </default>

  <worldbody>
